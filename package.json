{
  "private": true,
  "packageManager": "pnpm@7.3.0",
  "scripts": {
    "docs:dev": "pnpm --filter @my-component/docs dev",
    "docs:build": "pnpm --filter @my-component/docs build",
    "test": "pnpm --filter my-component test",
    "test:watch": "pnpm --filter my-component test:watch",
    "prebuild": "pnpm --filter my-component prebuild",
    "build:all": "pnpm --filter my-component build",
    "build:core": "pnpm --filter my-component build:core",
    "build:types": "pnpm --filter my-component build:types",
    "build:css": "pnpm --filter my-component build:css",
    "play:dev": "pnpm --filter @my-component/play dev"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@my-component/core": "workspace:my-component@",
    "@types/node": "^18.13.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/test-utils": "^2.2.10",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "jsdom": "^21.1.0",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.2",
    "rollup": "^3.17.3",
    "rollup-plugin-multi-input": "^1.3.3",
    "rollup-plugin-styles": "^4.0.0",
    "typescript": "^4.9.5",
    "unplugin-vue-macros": "^1.7.3",
    "vite": "^4.1.1",
    "vitest": "^0.28.5",
    "vue-tsc": "^1.0.24"
  }
}
